{
  "name": "gazetta",
  "version": "0.0.1",
  "scripts": {
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "build": "ng build --aot",
    "postinstall": "ng build --aot --prod",
    "pretty": "prettier --parser typescript --loglevel warn --write \"src/**/*.ts\"",
    "serve:local": "ng serve --aot --port 4201 --configuration=local",
    "serve:prod": "ng serve --aot --port 4201 --configuration=prod",
    "serve:ngrok": "ngrok http 4204",
    "start": "node server.js",
    "stf": "stf --ph=hostname --src=./src/environments/environment.sample --dest=./src/environments/environment.local.ts",
    "test": "ng test --code-coverage --watch=true",
    "test:once": "ng test --code-coverage --watch=false --progress=false --karma-config karma.ci.conf.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.9",
    "@angular/cdk": "^7.3.5",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.3.4",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "express": "^4.16.4",
    "path": "^0.12.7",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "xml-js": "^1.6.11",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.4",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngrok": "^3.1.1",
    "protractor": "~5.4.0",
    "sample-to-file": "^1.0.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-defocus": "^2.0.6",
    "typescript": "~3.2.2"
  },
  "engines": {
    "node": "11.10.0",
    "npm": "6.9.0"
  }
}
